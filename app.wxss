/* 导入通用的样式 */
@import './assets/style/alIconFont/aliconfont.wxss';
/* 小程序不支持通配符 */
page,view,text,swiper-item,img,navigator{
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}
