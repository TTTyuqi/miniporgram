  border-radius: 15px;
  background-color: #F6F7F8;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.seach-icon{
  font-size:14px !important;
  margin: 0 8px;
  color: #1296db;
}
.seache-info{
  color: #ccc;
  font-size: 12px;
}